    <div id="error_poruke_container_1" class="error_poruke_container">
        <script>
            var error_poruke = { polje_obavezno: "Поље је обавезно", neocekivani_format: "Поље није у очекиваном формату" };
        </script>
    </div>
    

      <script>
          $(function ()
          {
              if (navigator.userAgent.indexOf('Mac OS X') != -1) { $('body').addClass('mac'); } else { $('body').addClass('pc') };
              var ua = navigator.userAgent.toLowerCase();
              var isAndroid = ua.indexOf("android") > -1; if (isAndroid) { $('body').addClass('android'); }

              $('.header .meni a:not(.txtright a)').click(function () {
                  $('.meni').removeClass('aktivan');

                  $(this).parents().filter('.meni').addClass('aktivan');  
              });

              initErrorMessages();

              $('.mobilenavbutton').click(function () {
                  $('.mobilenavopen').addClass('aktivan'),
                  $('body').addClass('noscroll');
              });
              $('#closenavigation').click(function () {
                  $('.mobilenavopen').removeClass('aktivan'),
                  $('body').removeClass('noscroll');
              });

              

          });

          function initErrorMessages() {

              app.Config.ErrorPoruke = error_poruke;
              $('.Status1 [err_obavezno_polje_poruka2], .Status1 [los_regex_format2]').removeAttr('err_obavezno_polje_poruka2').removeAttr('los_regex_format2');
              $('.Status2 [err_obavezno_polje_poruka1], .Status2 [los_regex_format1]').removeAttr('err_obavezno_polje_poruka1').removeAttr('los_regex_format1');

              $('.Status1 [err_obavezno_polje_poruka1]').each(function () {
                  $(this).attr('err_obavezno_polje_poruka', $(this).attr('err_obavezno_polje_poruka1')).removeAttr('err_obavezno_polje_poruka1');
              });
              $('.Status2 [err_obavezno_polje_poruka2]').each(function () {
                  $(this).attr('err_obavezno_polje_poruka', $(this).attr('err_obavezno_polje_poruka2')).removeAttr('err_obavezno_polje_poruka2');
              });

              $('.Status1 [los_regex_format1]').each(function () {
                  $(this).attr('los_regex_format', $(this).attr('los_regex_format1')).removeAttr('los_regex_format1');
              });
              $('.Status2 [los_regex_format2]').each(function () {
                  $(this).attr('los_regex_format', $(this).attr('los_regex_format2')).removeAttr('los_regex_format2');
              });


          }
          function lpad(value, padding) {
              if (value == "-1") value = 0;
              if (value == null) value = 0;
              var zeroes = new Array(padding + 1).join("0");
              return (zeroes + value).slice(-padding);
          }
        </script>   

</body>
</html>
