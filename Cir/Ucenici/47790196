                    $('[dbf="Sifra"]').focus();

                    $('.Sesti span[dbf]:contains(0), .Sedmi span[dbf]:contains(0), .Osmi span[dbf]:contains(0)').each(function() { if ($(this).html() == '0') $(this).parents().filter('.trecinivo').addClass('invisible');  });

                });

                function PokaziDetalje(klasa, el) {
                    $('.aktlink').removeClass('aktlink');
                    $('.vidisekcija').removeClass('akt');
                   
                   
                    setTimeout(function () { 
                        
                        $('.'+klasa).addClass('akt');                      
                        $(el).parents().filter('.row.prikaz').addClass('aktlink');
                    }, 0);
                }

                function ZatvoriDetalje(klasa, el) {
                    $('.aktlink').removeClass('aktlink');
                    $('.vidisekcija').removeClass('akt');
                  
                }
                
                function IspisiUcenika() {

                    //var captchaContainer = null;
                    //var loadCaptcha = function() {
                    //    captchaContainer = grecaptcha.render('captcha_container', {
                    //        'sitekey' : '6LfHWyQUAAAAAMNpeNkLk3uPZyWc6362BpXY-wrz',
                    //        'lang' : 'sr',
                    //        'callback' : function(response) {
                                
                                $('.pretragaUcenik :dbf(Sifra):visible').attr('obavezan', 'obavezan');
                                $('.pretragaUcenik :dbf(Sifra):visible').izlaz();
                                if (!$('.pretragaUcenik').validacija()) {
                                    $('.pretragaUcenik :dbf(Sifra):visible').removeAttr('obavezan');
                                    return;
                                }
                                IDUcenik = $('.pretragaUcenik :dbf(Sifra):visible').val();
                                window.location = "/Ucenici/" + IDUcenik;
                    //        }
                    //    });
                    //};
                    //loadCaptcha(); 
                }
                function pravilnik(el){
                    window.location = "/Pravilnici/"+el;
                }

               
               


