                $(function () {
                    if(ucenik.length > 0) { 
                        $('.pozadinasekcije').initDBFsByObject(ucenik[0]);
                        $('.prazan_grid1').remove();
                        $('.prazan_grid2').removeClass('invisible');
                        if (parseFloat(ucenik[0].BodovaAM) == 0) $('.amBodovi').remove(); 
                        if (parseFloat(ucenik[0].BodovaUkupno) == -1) $('.manjeOd50, .viseOd50').remove(); 
                        if (parseFloat(ucenik[0].BodovaUkupno) < 50 && parseFloat(ucenik[0].BodovaUkupno) > 0) $('.nijeZavrsioOsnovnu, .viseOd50').remove(); 
                        if (parseFloat(ucenik[0].BodovaUkupno) >= 50) $('.manjeOd50, .nijeZavrsioOsnovnu').remove(); 
                        if (ucenik[0].blizanac == '' && ucenik[0].blizanac2 == '') $('.neBlizanackoDeteInvisible').remove();
                        else $('.blizanackoDeteInvisible').remove();
                        if (zelje2.length == 0) $('.NemaDrugeZeljeUkloni').remove();
                        if (zelje2.length > 0 && ucenik[0].blizanac != '' && ucenik[0].blizanac2 == '') $('.drugiKrugSopstveniBodoviUkloni').remove();
                        if (zelje2.length > 0 && ucenik[0].blizanac != '' && ucenik[0].blizanac2 != '') $('.drugiKrugTudjiBodoviUkloni').remove();
                    }
                   
                    if(sesti.length > 0) { $('.Sesti').initDBFsByObject(sesti[0]);}
                    if(sedmi.length > 0) { $('.Sedmi').initDBFsByObject(sedmi[0]);}
                    if(osmi.length > 0) { $('.Osmi').initDBFsByObject(osmi[0]);}
                    else $('.oceneosmi').remove();

                    for (var it= 0; it < nagrade.length; it++){
                        $('.Takmicenja .beznagrade').remove();
                        var html = $('.templateNagrada').html();
                        var novi = $(html).insertBefore($('.Takmicenja .ukupnoBodova'));
                        $(novi).initDBFsByObject(nagrade[it]);
                    }

                    //for (var it= 0; it < profili.length; it++){
